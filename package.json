{
	"name": "@titanium/barcode",
	"version": "1.0.12",
	"description": "⭐  Axway Amplify module for scanning barcodes with Appcelerator Titanium SDK",
	"main": "index.js",
	"scripts": {
		"postinstall": "tiapp-install-module",
		"postuninstall": "tiapp-uninstall-module",
		"lint": "eslint . --fix",
		"list": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
		"prep": "npm install --ignore-scripts && npm run list",
		"test": "npm run prep",
		"prepublishOnly2": "git pull && npm test && bump insane",
		"prepublishOnly": "git pull && npm test && npm audit fix && bump insane"
	},
	"author": "Axway",
	"maintainers": [
		{
			"name": "Brenton House",
			"email": "brenton.house@gmail.com",
			"url": "https://brenton.house"
		}
	],
	"repository": {
		"type": "git",
		"url": "github:brentonhouse/titanium-barcode"
	},
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/brentonhouse/titanium-barcode/issues"
	},
	"homepage": "https://brenton.house/saying-goodbye-to-axway-amplify-titanium-31a44f3671de",
	"keywords": [
		"titanium",
		"mobile",
		"alloy",
		"turbo",
		"axway",
		"appcelerator",
		"ios",
		"android",
		"iphone",
		"ipad",
		"barcode",
		"titanium-native-module",
		"sdk",
		"plugin",
		"amplify",
		"api-builder",
		"appcelerator-titanium",
		"titanium-sdk",
		"titanium-mobile",
		"titanium-module",
		"axway-sdk",
		"axway-amplify",
		"brenton-house"
	],
	"titanium": {
		"type": "native-module",
		"platform": {
			"ios": {
				"moduleid": "ti.barcode",
				"name": "barcode",
				"version": "2.0.4",
				"apiversion": 2,
				"architectures": [
					"armv7",
					"arm64",
					"i386",
					"x86_64"
				],
				"minsdk": "5.5.1",
				"guid": "fe2e658e-0eaf-44a6-b6d1-c074d6b986a3"
			},
			"android": {
				"moduleid": "ti.barcode",
				"name": "barcode",
				"version": "4.0.2",
				"apiversion": 4,
				"architectures": [
					"arm64-v8a",
					"armeabi-v7a",
					"x86"
				],
				"minsdk": "7.0.0",
				"guid": "fe2e658e-0eaf-44a6-b6d1-c074d6b986a3"
			}
		}
	},
	"dependencies": {
		"@titanium/tiapp-xml": ">=1.0.11"
	}
}
